{
  "name": "elara-rac-back",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d src/data_source.ts src/migration/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run -d src/data_source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/data_source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.24",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.21",
    "@types/winston": "^2.4.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
